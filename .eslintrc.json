{
  "extends": [
    "next",
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@next/next/no-page-custom-font": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["."],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      },
      "alias": {
        "map": [["@", "."]],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
